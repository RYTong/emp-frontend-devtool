{
  "name": "emp-frontend-devtool",
  "main": "./lib/main",
  "version": "1.2.4",
  "description": "EMP Frontend Development Environment",
  "keywords": [],
  "repository": "https://github.com/RYTong/emp-frontend-devtool",
  "engines": {
    "atom": ">=1.13.1 <2.0.0"
  },
  "scripts": {
    "test": "standard"
  },
  "standard": {
    "globals": [
      "atom"
    ]
  },
  "providedServices": {
    "atom.file-icons": {
      "versions": {
        "1.0.0": "onFileVisibleInTree"
      }
    },
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "package-deps": [
    "tool-bar",
    "file-icons",
    "linter",
    "autocomplete-emp-lua",
    "highlight-selected",
    "language-lua"
  ],
  "dependencies": {
    "atom-message-panel": "^1.2.7",
    "atom-package-deps": "^4.3.1",
    "atom-space-pen-views": "*",
    "async": "^0.9.0",
    "body-parser": "^1.15.2",
    "detect-port": "^1.0.5",
    "express": "^4.14.0",
    "fs-extra": "^1.0.0",
    "fuzzaldrin": "^2.1",
    "html-escape": "^2.0.0",
    "jquery": "^3.1.0",
    "less": "^2.3.1",
    "lodash": "^4.15.0",
    "loophole": "^1.1.0",
    "luaparse": "^0.2.1",
    "minimatch": "~0.3.0",
    "mkdirp": "^0.5.0",
    "open": "0.0.5",
    "qr-image": "^3.1.0",
    "rc-banner-anim": "^0.5.4",
    "rc-tween-one": "^0.11.11",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-player": "^0.14.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "shelljs": "^0.7.4",
    "underscore-plus": "1.x",
    "fs-plus": "*"
  },
  "devDependencies": {
    "standard": "^8.6.0"
  }
}
